{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "outputs": [
        "node_modules/.cache/storybook/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "storybook-static/**"
      ],
      "env": [
        "NODE_ENV",
        "PEPPER"
      ]
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
